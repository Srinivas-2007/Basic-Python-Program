# +ve, -ve, zero
a=int(input('enter a number: '))
if(a>0):
    print("positive")
elif(a<0):
    print("negative")
else:
    print("zero")

#even or odd numbers
a=int(input("enter a no. :"))
if(a%2==0):
    print("even no. ")
else:
    print("odd no. ")

#to check if the number is divisible by 5 &11
a=int(input("enter a number: "))
if(a%5==0 and a%11==0):
    print("number is divisible by both 5 & 11")
else:
    print("number will not divivsible by 5 & 11")

# ATM CARD 
ATM_card=input(" ")
Pin= input(" password: ")
status = input(" ")
if(ATM_card == "inserted" and Pin== "PASS" and status=="not locked"):
    print("access granted")
else:
    print("access denied")

#student pass in an exam
theory=int(input())
practical=int(input())
total= int(input())
if(theory>=35 and practical>=50 and total>=60):
    print("PASS")
else:
    print("FAIL")

#check age group
age=int(input("enter your age: "))
if(age<=12):
    print("he is child")
elif(age>12 and age<=19):
    print("he is teenager")
else:
    print("he is adult")

#to check grade using marks
marks=int(input("enter marks: "))
if(marks>=90):
    print("A-grade")
    print("excellent")
elif(marks<=89 or marks>70):
    print("B-grade")
    print("best")
elif(marks>50 or marks<=70):
    print("C-grade")
    print("good")
elif(marks>40 or marks<=50):
    print("D-grade")
    print("need improvement")
elif(marks<40):
    print("FAIL")
else:
    print("error")

#to check week of a day
day = int(input("enter day: "))
if(day==1):
    print("sunday")
elif(day==2):
    print("monday")
elif(day==3):
    print("tuesday")
elif(day==4):
    print("wednesday")
elif(day==5):
    print("thursday")
elif(day==6):
    print("friday")
elif(day==7):
    print("saturday")
else:
    print("error")

