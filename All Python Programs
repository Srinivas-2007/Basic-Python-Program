#swap two numebrs wtihout 3rd variable
a=int(input("enter 1st no. "))
b=int(input("enter 2nd no."))
a=a+b
b=a-b
a=a-b
print(a)
print(b)

#swap 2 numbers using 3rd variable
a=int(input("1st no. :"))
b=int(input("enter 2nd no. :"))
temp=a
a=b
b=temp
print(a)
print(b)

#primitive datatypes( int, floating, boolean)
