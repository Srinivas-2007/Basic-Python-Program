else:
    print("wrong credentials")

# develop a simple calculator
a=int(input("enter 1st no.: "))
b=int(input("enter 2nd no.: "))
operation=input("enter operation: ")
if(operation=="add"):
    print(a+b)
elif(operation=="subtract"):
    print(a-b)
elif(operation=='multiply'):
    print(a*b)
elif(operation=='division'):
    print(a/b)
elif( operation=='mod div'):
    print(a%b)
else:
    print('error')

#to check the month days 
num = int(input("enter month: "))
if(num==1,3,5,7,8,10,12):
    print("31 days")
elif(num == 2):
    print("28 or 29 days")
elif(num == 4,6,9,11):
    print("30 days")
else:
    print("error")

#to check body temperature 
temp=float(input("enter temperature: "))
if(temp==37):
    print("body temperature is normal")
elif(temp>37):
    print("fever")
else:
    print("Cold")

# sum of all cubes from m to n numbers
m=int(input("enter m no.: "))
n=int(input("enter n no.: "))
if(m>n):
    return0
else:
    sum=0
    for i in range(m,n+1):
        sum+= i**3
    print(sum)
