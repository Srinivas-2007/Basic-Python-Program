# product of all even integers from m to n

def product_of_even_numbers(m,n):
    if m>n :
        return -1
    product=1
    for i in range(m,n+1):
        if(i%2==0):
            product*=i
    return product
m=int(input("enter m: "))
n=int(input("enter n: "))
result=product_of_even_numbers(m,n)
print(result)
