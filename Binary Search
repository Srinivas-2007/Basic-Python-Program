# Binary Search

arr=[1,2,3,4,5,6,7,8,9]
def binary_search(arr,target):
    lb=0
    ub=len(arr)-1
    
    while lb<=ub:
        mid=(lb+ub)//2
        if(arr[mid]==target):
            print("target found at mid: ",mid)
            return mid
        elif(arr[mid]<target):
            lb=mid+1
        else:
            ub=mid-1
    print("element not found")
    return -1
binary_search(arr,8)
